                        WORK-CAN-O2-DATE WORK-COM-O2
              MOVE SPACES TO WORK-ADDON-FG WORK-CANCEL-FG.

      *    IF TP-CODE = "P"
              IF TP-ADDON-TYPE = "O2" AND
                       TP-ADDON-TEXT = "ADD"
                 MOVE "A" TO WORK-ADDON-FG
                 COMPUTE WORK-NEW-O2 = WORK-NEW-O2 +
                              (TP-APCUR - TP-GLAMT(1) - TP-GLAMT(2))
                 COMPUTE WORK-COM-O2 = WORK-COM-O2 + TP-INSCOMM
              ELSE
              IF TP-ADDON-TYPE = "O2" AND
                       TP-ADDON-TEXT = "ADJ"
                 COMPUTE WORK-ADJ-O2 = WORK-ADJ-O2 +
                              (TP-APCUR - TP-GLAMT(1) - TP-GLAMT(2))
                 COMPUTE WORK-COM-O2 = WORK-COM-O2 + TP-INSCOMM
              ELSE
              IF TP-ADDON-TYPE = "O2" AND
                       TP-ADDON-TEXT = "CNL"
                 MOVE "C" TO WORK-CANCEL-FG
                 COMPUTE WORK-CAN-O2 = WORK-CAN-O2 +
                              (TP-APCUR - TP-GLAMT(1) - TP-GLAMT(2))
                 MOVE TP-PAYDATE TO WORK-CAN-O2-DATE
                 COMPUTE WORK-COM-O2 = WORK-COM-O2 + TP-INSCOMM
              ELSE
              IF TP-TRCD = "R2"
                 COMPUTE WORK-CAN-O2 = WORK-CAN-O2 +
                                   ((TP-TRAMT - TP-INS-TAX-RB) * -1)
                 MOVE TP-PAYDATE TO WORK-CAN-O2-DATE
                 COMPUTE WORK-COM-O2 = WORK-COM-O2 + (TP-INSCOMM * -1)
              ELSE
              IF TP-TRCD = "C2"
                 MOVE "C" TO WORK-CANCEL-FG
                 COMPUTE WORK-CAN-O2 = WORK-CAN-O2 +
                                   ((TP-TRAMT - TP-INS-TAX-RB) * -1)
                 MOVE TP-PAYDATE TO WORK-CAN-O2-DATE
                 COMPUTE WORK-COM-O2 = WORK-COM-O2 + (TP-INSCOMM * -1).

           IF IO-FG = 0
              PERFORM REWRITE-WK-FILE
           ELSE
              PERFORM WRITE-WK-FILE.

           GO TO SORT-TRP-NEXT.

