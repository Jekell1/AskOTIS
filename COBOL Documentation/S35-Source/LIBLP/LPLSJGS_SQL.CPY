      *-----------------------------------------------------------------
      *    LOAD LSJ-REC FROM QLSJ-REC; FOR READ & READ-NEXT
      *-----------------------------------------------------------------
       GET-LSJ-FIELDS.
           MOVE QLSJ-BRNO TO LSJ-BRNO.
           MOVE QLSJ-ACCTNO TO LSJ-ACCTNO.
           MOVE QLSJ-SEQNO TO LSJ-SEQNO.

           IF ( QLSJ-LTOUCH-DATE = "1900-01-01" )
              MOVE ZEROES TO LSJ-LTOUCH-DATE
           ELSE
              MOVE QLSJ-LTOUCH-DATE TO SQL-DATE-YYYY-MM-DD
              PERFORM SQL-GET-DATE
              MOVE SQL-DATE-YYYYMMDD TO LSJ-LTOUCH-DATE.


           IF ( QLSJ-FILEDATE = "1900-01-01" )
              MOVE ZEROES TO LSJ-FILEDATE
           ELSE
              MOVE QLSJ-FILEDATE TO SQL-DATE-YYYY-MM-DD
              PERFORM SQL-GET-DATE
              MOVE SQL-DATE-YYYYMMDD TO LSJ-FILEDATE.

           MOVE QLSJ-CASE-NO TO LSJ-CASE-NO.
           MOVE QLSJ-AMT TO LSJ-AMT.
           MOVE QLSJ-COUNTY TO LSJ-COUNTY.
           MOVE QLSJ-CLERK-NAME TO LSJ-CLERK-NAME.
           MOVE QLSJ-CLERK-ADDR TO LSJ-CLERK-ADDR.
           MOVE QLSJ-CLERK-CITY TO LSJ-CLERK-CITY.
           MOVE QLSJ-CLERK-STATE TO LSJ-CLERK-STATE.
           MOVE QLSJ-CLERK-ZIP TO LSJ-CLERK-ZIP.
           MOVE QLSJ-CLERK-PHONE TO LSJ-CLERK-PHONE.
           MOVE QLSJ-SHERIFF-FEE TO LSJ-SHERIFF-FEE.
           MOVE QLSJ-COURT-FEE TO LSJ-COURT-FEE.
           MOVE QLSJ-ATTY-FEE TO LSJ-ATTY-FEE.

           IF ( QLSJ-DISMISS-DATE = "1900-01-01" )
              MOVE ZEROES TO LSJ-DISMISS-DATE
           ELSE
              MOVE QLSJ-DISMISS-DATE TO SQL-DATE-YYYY-MM-DD
              PERFORM SQL-GET-DATE
              MOVE SQL-DATE-YYYYMMDD TO LSJ-DISMISS-DATE.

           MOVE QLSJ-GARN-INFO TO LSJ-GARN-INFO.
           MOVE QLSJ-EMPLOY-NAME TO LSJ-EMPLOY-NAME.
           MOVE QLSJ-EMPLOY-ADDR TO LSJ-EMPLOY-ADDR.
           MOVE QLSJ-EMPLOY-CITY TO LSJ-EMPLOY-CITY.
           MOVE QLSJ-EMPLOY-STATE TO LSJ-EMPLOY-STATE.
           MOVE QLSJ-EMPLOY-ZIP TO LSJ-EMPLOY-ZIP.

           IF ( QLSJ-GARN-DATE = "1900-01-01" )
              MOVE ZEROES TO LSJ-GARN-DATE
           ELSE
              MOVE QLSJ-GARN-DATE TO SQL-DATE-YYYY-MM-DD
              PERFORM SQL-GET-DATE
              MOVE SQL-DATE-YYYYMMDD TO LSJ-GARN-DATE.

           MOVE QLSJ-JD-FILED-ON TO LSJ-JD-FILED-ON.
           MOVE QLSJ-GARN-FILED-ON TO LSJ-GARN-FILED-ON.
           MOVE QLSJ-ATTY-NAME TO LSJ-ATTY-NAME.
           MOVE QLSJ-ATTY-ADDR TO LSJ-ATTY-ADDR.
           MOVE QLSJ-ATTY-CITY TO LSJ-ATTY-CITY.
           MOVE QLSJ-ATTY-STATE TO LSJ-ATTY-STATE.
           MOVE QLSJ-ATTY-ZIP TO LSJ-ATTY-ZIP.
           MOVE QLSJ-ATTY-PHONE TO LSJ-ATTY-PHONE.
 
      *-----------------------------------------------------------------
      *    LOAD QLSJ-REC FROM LSJ-REC; FOR WRITE & REWRITE
      *-----------------------------------------------------------------
       SET-LSJ-FIELDS.
           MOVE LSJ-BRNO TO QLSJ-BRNO.
           MOVE LSJ-ACCTNO TO QLSJ-ACCTNO.
           MOVE LSJ-SEQNO TO QLSJ-SEQNO.

           IF ( LSJ-LTOUCH-DATE = ZEROES )
