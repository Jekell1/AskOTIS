           MOVE LSJ-CLERK-ZIP TO QLSJ-CLERK-ZIP.
           MOVE LSJ-CLERK-PHONE TO QLSJ-CLERK-PHONE.
           MOVE LSJ-SHERIFF-FEE TO QLSJ-SHERIFF-FEE.
           MOVE LSJ-COURT-FEE TO QLSJ-COURT-FEE.
           MOVE LSJ-ATTY-FEE TO QLSJ-ATTY-FEE.

           IF ( LSJ-DISMISS-DATE = ZEROES )
              MOVE "1900-01-01" TO QLSJ-DISMISS-DATE
           ELSE
              MOVE LSJ-DISMISS-DATE TO SQL-DATE-YYYYMMDD
              PERFORM SQL-SET-DATE
              MOVE SQL-DATE-YYYY-MM-DD TO QLSJ-DISMISS-DATE.

           MOVE LSJ-GARN-INFO TO QLSJ-GARN-INFO.
           MOVE LSJ-EMPLOY-NAME TO QLSJ-EMPLOY-NAME.
           MOVE LSJ-EMPLOY-ADDR TO QLSJ-EMPLOY-ADDR.
           MOVE LSJ-EMPLOY-CITY TO QLSJ-EMPLOY-CITY.
           MOVE LSJ-EMPLOY-STATE TO QLSJ-EMPLOY-STATE.
           MOVE LSJ-EMPLOY-ZIP TO QLSJ-EMPLOY-ZIP.

           IF ( LSJ-GARN-DATE = ZEROES )
              MOVE "1900-01-01" TO QLSJ-GARN-DATE
           ELSE
              MOVE LSJ-GARN-DATE TO SQL-DATE-YYYYMMDD
              PERFORM SQL-SET-DATE
              MOVE SQL-DATE-YYYY-MM-DD TO QLSJ-GARN-DATE.

           MOVE LSJ-JD-FILED-ON TO QLSJ-JD-FILED-ON.
           MOVE LSJ-GARN-FILED-ON TO QLSJ-GARN-FILED-ON.
           MOVE LSJ-ATTY-NAME TO QLSJ-ATTY-NAME.
           MOVE LSJ-ATTY-ADDR TO QLSJ-ATTY-ADDR.
           MOVE LSJ-ATTY-CITY TO QLSJ-ATTY-CITY.
           MOVE LSJ-ATTY-STATE TO QLSJ-ATTY-STATE.
           MOVE LSJ-ATTY-ZIP TO QLSJ-ATTY-ZIP.
           MOVE LSJ-ATTY-PHONE TO QLSJ-ATTY-PHONE.
 

